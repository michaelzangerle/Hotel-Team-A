<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <joined-subclass name="projekt.fhv.teama.classes.personen.Gast"
    				 extends="projekt.fhv.teama.classes.personen.Person">
        
        <key column="gastID"/>
        
        <property name="nummer" type="java.lang.String">
            <column name="NUMMER" sql-type="long"/>
        </property>
    </joined-subclass>
</hibernate-mapping>