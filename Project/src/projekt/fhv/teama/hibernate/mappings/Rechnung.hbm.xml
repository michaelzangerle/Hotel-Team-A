<?xml version='1.0' encoding='utf-8'?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
        xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping http://www.hibernate.org/xsd/hibernate-mapping/hibernate-mapping-4.0.xsd" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <class name="Entities.Rechnung" table="rechnung" schema="" catalog="hibernate">
        <id name="rechnungId">
            <column name="rechnungID" sql-type="int" length="10" not-null="true"/>
        </id>
        <property name="bezahlerVorname">
            <column name="bezahlerVorname" sql-type="varchar" not-null="true"/>
        </property>
        <property name="bezahlerNachname">
            <column name="bezahlerNachname" sql-type="varchar" not-null="true"/>
        </property>
        <property name="bezahlerStrasse">
            <column name="bezahlerStrasse" sql-type="varchar" not-null="true"/>
        </property>
        <property name="bezahlerOrt">
            <column name="bezahlerOrt" sql-type="varchar" not-null="true"/>
        </property>
        <property name="bezahlerPlz">
            <column name="bezahlerPLZ" sql-type="varchar" not-null="true"/>
        </property>
        <property name="summe">
            <column name="summe" sql-type="decimal" length="12" precision="2" not-null="true"/>
        </property>
        <property name="nummer">
            <column name="nummer" sql-type="varchar" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>