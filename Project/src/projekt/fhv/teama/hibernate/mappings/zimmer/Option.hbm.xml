<?xml version='1.0' encoding='utf-8'?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
        xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping http://www.hibernate.org/xsd/hibernate-mapping/hibernate-mapping-4.0.xsd" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <class name="Entities.Option" table="option" schema="" catalog="hibernate">
        <id name="optionId">
            <column name="optionID" sql-type="int" length="10" not-null="true"/>
        </id>
        <property name="erfuellung">
            <column name="erfuellung" sql-type="date" length="10" not-null="true"/>
        </property>
        <property name="prozentsatz">
            <column name="prozentsatz" sql-type="smallint" length="5" not-null="true"/>
        </property>
        <property name="erfuellt">
            <column name="erfuellt" sql-type="bit" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>