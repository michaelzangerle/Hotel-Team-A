<?xml version='1.0' encoding='utf-8'?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
        xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping http://www.hibernate.org/xsd/hibernate-mapping/hibernate-mapping-4.0.xsd" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <class name="Entities.Teilzahlung" table="teilzahlung" schema="" catalog="hibernate">
        <composite-id mapped="true" class="Entities.TeilzahlungPK">
            <key-property name="zahlungsmethodeId">
                <column name="zahlungsmethodeID" sql-type="int" length="10" not-null="true"/>
            </key-property>
            <key-property name="rechnungId">
                <column name="rechnungID" sql-type="int" length="10" not-null="true"/>
            </key-property>
        </composite-id>
        <property name="betrag">
            <column name="betrag" sql-type="decimal" length="12" precision="2" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>